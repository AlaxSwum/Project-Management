# PRODUCTION ENVIRONMENT VARIABLES
# Copy this file to .env.production and update with your actual production URLs

# API Configuration
NEXT_PUBLIC_API_URL=https://your-backend-subdomain.yourdomain.com/api
NEXT_PUBLIC_AUTH_URL=https://your-backend-subdomain.yourdomain.com/api/auth

# App Configuration
NEXT_PUBLIC_APP_URL=https://yourdomain.com
NEXT_PUBLIC_APP_NAME=Project Management System

# Google Drive API (for file uploads)
NEXT_PUBLIC_GOOGLE_DRIVE_API_KEY=your_google_drive_api_key
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your_google_client_id

# Analytics (Optional)
NEXT_PUBLIC_GA_ID=your-google-analytics-id 